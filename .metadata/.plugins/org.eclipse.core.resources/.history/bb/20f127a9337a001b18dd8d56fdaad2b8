package day13;

public class C1_Concatination {

	public static void main(String[] args) {
		// concatenation
		String str1 = "Hello";
		String str2 = "World";
	
	System.out.println(str1.concat(str2));
	System.out.println(str1.concat(" ").concat(str2)); //arada bosluk koyduk
											
	//charAt(index) 
	
	
	 	 
	 
	 
	
	 
	
	}

}
